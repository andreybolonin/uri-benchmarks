{
    "path": "benchmarks",
    "bootstrap": "vendor/autoload.php",
    "reports": {
        "uri_compare": {
            "extends": "aggregate",
            "compare": "benchmark",
            "compare_fields": [ "mode" ],
            "cols": [ "subject" ],
            "labels": [ "Subject", "VDB", "Aura", "Dicrators", "NetUrl", "ZendUri", "League", "Guzzle", "Slime", "Windwalker" ]
        },
        "uri_aggregate": {
            "extends": "aggregate",
            "cols": [ "benchmark", "subject", "mem_peak", "best", "mean", "mode", "worst", "rstdev", "diff" ],
            "break": ["subject"]
        }
    }
}
